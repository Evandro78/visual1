Algoritmo "SOMALANCHE"

Var
  precosalgado, precobebida, total: real

Inicio


   escreval("preço salgado")
   Leia (precosalgado)

   escreval("digite sua nota2")
   leia (precobebida)

   escreval("digite sua nota3")
   leia (total)

   total <-  precosalgado +  precobebida
      escreval("o preço é:" , total)


Fimalgoritmo